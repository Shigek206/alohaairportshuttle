RewriteEngine On
Options -MultiViews


RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.php [NC,L]

RewriteCond %{http_host} ^alohaairportshuttle.com [NC]
RewriteRule ^(.*)$ http://www.alohaairportshuttle.com/$1 [R=301,L] 

RewriteCond %{SERVER_PORT} 80
RewriteCond %{HTTP_HOST} ^(www\.)?alohaairportshuttle\.com
RewriteRule ^(.*)$ https://www.alohaairportshuttle.com/$1 [R,L]